<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="net.wanji.dataset.mapper.InspectionStatusMapper">
    <update id="updateInspectionStatus" parameterType="net.wanji.dataset.entity.InspectionStatus">
        alter table  high_velocity_free_flow.inspection_status update
            <if test="beTrue != null">be_true=#{beTrue},</if>
            <if test="checkCondition != null">check_condition=#{checkCondition}</if>
        where record_id = #{recordId}
    </update>
</mapper>
